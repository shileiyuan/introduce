{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      },
      "react-hot-loader/babel",
      "add-module-exports"
    ],
    "transform-object-rest-spread",
    "transform-decorators-legacy"
  ]
}
